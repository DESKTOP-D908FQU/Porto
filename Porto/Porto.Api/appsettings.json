{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "DefaultApiVersion": {
    "Major": 1,
    "Minor": 0,
    "ReportApiVersions": true,
    "AssumeDefaultVersionWhenUnspecified": true,
    "GroupNameFormat": "'v'VVVV",
    "SubstituteApiVersionInUrl": true
  }
}
